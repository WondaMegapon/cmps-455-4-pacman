{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"problemMatcher": [
				"$rustc"
			],
			"label": "Normal ol' Cargo",
			"group": {
				"kind": "build",
			}
		},
		{
			"type": "shell",
			"command": "nix-shell --run \"cargo run\"",
			"label": "Shelled Nix",
			"group": {
				"kind": "build",
			}
		}
	]
}